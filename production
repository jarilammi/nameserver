[nameservers]
ns1 ansible_host=192.168.1.254 webserver_ip=192.168.1.250 loadbalancer_ip=192.168.1.252
ns2 ansible_host=172.16.2.254 webserver_ip=172.16.2.250 loadbalancer_ip=172.16.2.252

[loadbalancers]
lb1 ansible_host=192.168.1.252
lb2 ansible_host=172.16.2.252

[webservers]
fe1 ansible_host=192.168.1.250
fe2 ansible_host=172.16.2.250
